{
  "name": "@node-real/walletkit",
  "version": "2.2.0",
  "author": "node-real",
  "private": false,
  "description": "WalletKit is a React component library for easily connecting a wallet to your dApp.",
  "homepage": "https://node-real.github.io/walletkit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/node-real/walletkit.git",
    "directory": "packages/walletkit"
  },
  "keywords": [
    "walletkit"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/style.css",
    "./wagmi": {
      "types": "./dist/wagmi/index.d.ts",
      "default": "./dist/wagmi/index.js"
    },
    "./wagmi/wallets": {
      "types": "./dist/wagmi/wallets.d.ts",
      "default": "./dist/wagmi/wallets.js"
    },
    "./solana": {
      "types": "./dist/solana/index.d.ts",
      "default": "./dist/solana/index.js"
    },
    "./solana/wallets": {
      "types": "./dist/solana/wallets.d.ts",
      "default": "./dist/solana/wallets.js"
    }
  },
  "scripts": {
    "dev": "pnpm cp:css && vite __dev__ --config vite.config.ts --port 3332 --host 0.0.0.0 --open",
    "cp:css": "cat ../wagmi/dist/style.css ../solana/dist/style.css > __dev__/style.css",
    "build": "vite build"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.51.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.5.3",
    "vconsole": "^3.15.1",
    "viem": "^2.17.4",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^3.9.1",
    "wagmi": "^2.10.10",
    "@solana/web3.js": "^1",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@walletconnect/solana-adapter": "^0.0.5",
    "qrcode": "^1.5.3"
  },
  "dependencies": {
    "@node-real/walletkit-wagmi": "workspace:*",
    "@node-real/walletkit-solana": "workspace:*"
  }
}
