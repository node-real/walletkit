{
  "name": "walletkit",
  "version": "0.0.0",
  "author": "node-real",
  "private": true,
  "packageManager": "pnpm@8.9.2",
  "description": "Toolkit for connecting wallets.",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "lint": "pnpm eslint .",
